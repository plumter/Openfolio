{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@popperjs/core": "^2.11.6",
    "@successtar/cogo-toast": "^5.0.6",
    "@successtar/react-reveal": "^1.2.10",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.3.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-easy-crop": "^4.6.3",
    "react-inlinesvg": "^3.0.1",
    "react-loading-skeleton": "^3.1.1",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.8.1",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test:unit": "react-scripts test --passWithNoTests --watchAll=false --transformIgnorePatterns 'node_modules/(?!my-library-dir)/' --testPathPattern=src/__test__/Unit --bail 1 --silent --maxWorkers=2",
    "test:integration": "react-scripts test --passWithNoTests --watchAll=false --transformIgnorePatterns 'node_modules/(?!my-library-dir)/' --testPathPattern=src/__test__/Integration --bail 1 --silent --maxWorkers=2",
    "test:all": "react-scripts test --passWithNoTests --transformIgnorePatterns 'node_modules/(?!my-library-dir)/' --watchAll=false --testPathPattern=src/__test__ --bail 1 --silent --maxWorkers=2",
    "test:local": "react-scripts test --passWithNoTests --transformIgnorePatterns 'node_modules/(?!my-library-dir)/' --watchAll=false --testPathPattern=src/__test__ --bail 1",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "msw": "^1.0.1",
    "node-sass": "^8.0.0",
    "react-test-renderer": "^18.2.0",
    "tailwindcss": "^3.2.4"
  }
}
